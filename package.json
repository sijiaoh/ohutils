{
  "name": "osushi-with-auth",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "build": "yarn generate && next build",
    "start": "next start",
    "lint": "gts lint",
    "cleanGenerates": "rm -rf generated src/generated",
    "clean": "yarn cleanGenerates && rm -rf .next",
    "fix": "gts fix",
    "test": "yarn generate && NODE_PATH=. yarn jest && yarn lint",
    "execScript": "NODE_PATH=. ts-node --project=tsconfig.script.json",
    "generateGraphqlSchema": "yarn execScript scripts/generateGraphqlSchema",
    "generateGraphqlDefaultQueries": "yarn graphql-codegen --config=lib/default-queries.codegen.json",
    "generateGraphqlSrc": "yarn graphql-codegen --config=lib/src.codegen.json",
    "generateGraphql": "yarn generateGraphqlDefaultQueries && yarn generateGraphqlSrc",
    "generate": "yarn cleanGenerates && yarn generateGraphqlSchema && yarn generateGraphql",
    "prepareMysql": "yarn execScript scripts/prepareMysql",
    "removeDatabase": "yarn docker-mysql rm --userName ${DB_USER} --password ${DB_PASS} --volume ${DB_VERSION} ${DB_NAME}",
    "resetDatabase": "yarn removeDatabase && yarn prepareMysql",
    "execMysql": "yarn execScript scripts/execMysql"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@emotion/react": "^11.5.0",
    "@reactive-class/react": "^0.1.1",
    "class-validator": "^0.13.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.7.0",
    "mysql2": "^2.3.2",
    "next": "12.0.1",
    "next-connect": "^0.11.0",
    "nookies": "^2.5.2",
    "passport": "^0.5.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.38"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.15.4",
    "@emotion/babel-plugin": "^11.3.0",
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-graphql-apollo": "^1.0.0",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@graphql-codegen/typescript-react-apollo": "^3.1.6",
    "@sijiaoh/docker-mysql": "^0.1.0",
    "@sijiaoh/graphql-codegen-default-queries": "^0.0.2",
    "@types/cookie": "^0.4.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth20": "^2.0.10",
    "@types/react": "17.0.33",
    "@types/uuid": "^8.3.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint-config-next": "12.0.1",
    "execa": "^5.1.1",
    "jest": "^27.3.1",
    "sijiaoh-gts": "^0.5.4",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "4.4.4",
    "uuid": "^8.3.2"
  }
}
