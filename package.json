{
  "name": "ohutils",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "build": "yarn generate && next build",
    "start": "next start",
    "lint": "gts lint",
    "cleanGenerates": "rm -rf generated src/generated test/generated",
    "clean": "yarn cleanGenerates && rm -rf .next",
    "fix": "gts fix",
    "jestOnly": "NODE_PATH=. yarn jest",
    "test": "yarn generate && yarn lint && yarn jestOnly && yarn build",
    "execScript": "NODE_PATH=. ts-node --project=tsconfig.script.json",
    "generateEnv": "yarn execScript scripts/generateEnv",
    "generateIndexes": "yarn ts-index-generator src/**/*.ts",
    "generateGraphqlSchema": "yarn execScript scripts/generateGraphqlSchema",
    "generateGraphqlDefaultQueries": "yarn graphql-codegen --errors-only --config=lib/default-queries.codegen.json",
    "generateGraphqlSrc": "yarn graphql-codegen --errors-only --config=lib/src.codegen.json",
    "generateGraphql": "yarn generateGraphqlDefaultQueries && yarn generateGraphqlSrc",
    "generate": "yarn generateEnv && yarn generateIndexes && yarn generateGraphqlSchema && yarn generateGraphql",
    "watchGenerate": "yarn nodemon --exec yarn generate",
    "prepareMysql": "yarn execScript scripts/prepareMysql",
    "removeDatabase": "yarn execScript scripts/removeDatabase",
    "resetDatabase": "yarn removeDatabase && yarn prepareMysql",
    "execMysql": "yarn execScript scripts/execMysql"
  },
  "dependencies": {
    "@apollo/client": "3.4.x",
    "@emotion/react": "^11.5.0",
    "@emotion/serialize": "^1.0.2",
    "@reactive-class/react": "^0.1.1",
    "bootstrap": "5.1.3",
    "class-validator": "^0.13.1",
    "date-fns": "^2.25.0",
    "express-graphql": "^0.12.0",
    "graphql": "^15.7.2",
    "highlight.js": "^11.3.1",
    "immer": "^9.0.6",
    "localforage": "^1.10.0",
    "marked": "^3.0.8",
    "mermaid": "^8.13.3",
    "mysql2": "^2.3.3-rc.0",
    "next": "^12.0.4",
    "next-connect": "^0.11.0",
    "nookies": "^2.5.2",
    "passport": "^0.5.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "17.0.2",
    "react-bootstrap": "^2.0.2",
    "react-dom": "17.0.2",
    "react-ga": "^3.3.0",
    "react-hook-form": "^7.19.1",
    "react-icons": "^4.3.1",
    "react-loader-spinner": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "0.2.39",
    "underscore": "^1.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@babel/plugin-proposal-private-methods": "^7.16.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.0",
    "@emotion/babel-plugin": "^11.3.0",
    "@graphql-codegen/cli": "^2.2.2",
    "@graphql-codegen/typescript": "^2.3.1",
    "@graphql-codegen/typescript-generic-sdk": "^2.2.0",
    "@graphql-codegen/typescript-graphql-apollo": "^1.1.0",
    "@graphql-codegen/typescript-operations": "^2.2.0",
    "@graphql-codegen/typescript-react-apollo": "^3.2.1",
    "@next/bundle-analyzer": "11.x",
    "@sijiaoh/docker-mysql": "^0.1.0",
    "@sijiaoh/graphql-codegen-default-queries": "^0.0.3",
    "@sijiaoh/ts-index-generator": "^0.0.2",
    "@types/cookie": "^0.4.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/marked": "^3.0.2",
    "@types/mermaid": "^8.2.7",
    "@types/node": "^16.11.7",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/react": "17.0.34",
    "@types/supertest": "^2.0.11",
    "@types/underscore": "^1.11.3",
    "@types/uuid": "^8.3.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "circular-dependency-plugin": "^5.2.2",
    "eslint-config-next": "^12.0.4",
    "execa": "^5.1.1",
    "express": "^4.17.1",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "sijiaoh-gts": "^0.6.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "4.4.4",
    "uuid": "^8.3.2"
  }
}
